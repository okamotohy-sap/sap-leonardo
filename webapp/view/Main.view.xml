<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:iot="sap.ui.iot.controls"
	xmlns:iotele="sap.ui.iot.elements" xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	controllerName="public.controller.Main" displayBlock="true">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}" backgroundDesign="Solid" showFooter="false">
				<content>
					<HBox width="100%" height="100%" id="__hbox0">
						<items>
							<VBox width="30%" direction="Column" id="__vbox0">
							    <items>
							        <iot:IoTThingList things="{path:'/Things',parameters:{expand:'DYN_ENT_com_sap_appiot_eventtypes__StandardEventType'}}" id="__list10" action:w5g.dt.context="/Things" rowSelect="action" action:wiring="\{'rowSelect':\{'targets':[\{'id':'__tmap0','action':'doMapZoom','parameters':\{'context':'thing'\}\}]\}\}" controlType="List">
							            <iot:columns>
							                <iotele:IoTThingListColumnHeader text="Column 1" id="__header6"/>
							                <iotele:IoTThingListColumnHeader text="Column 2" id="__header7"/>
							                <iotele:IoTThingListColumnHeader text="Column 3" id="__header8"/></iot:columns>
							            <iot:things><iotele:IoTThingListElement thingImage="{ThingId}" highSeverity="{DYN_ENT_com_sap_appiot_eventtypes__StandardEventType/High}" mediumSeverity="{DYN_ENT_com_sap_appiot_eventtypes__StandardEventType/Medium}" lowSeverity="{DYN_ENT_com_sap_appiot_eventtypes__StandardEventType/Low}" id="__element3">
							                    <iotele:cells>
							                        <iotele:IoTThingListColumn text="{ThingType}" id="__column16"/></iotele:cells>
							                </iotele:IoTThingListElement>
							            </iot:things></iot:IoTThingList>
							    </items>
							</VBox>
							<VBox width="70%" direction="Column" id="__vbox1">
							    <items>
							        <iot:IoTMap things="{path:'/Things',parameters:{expand:'DYN_ENT_com_sap_appiot_eventtypes__StandardEventType'}}" id="__tmap0" action:wiring="\{\}" geoCirclePress="" pinPress="">
							            <iot:things>
							                <iotele:IoTMapElement thingid="{ThingId}" longitude="{Longitude}" latitude="{Latitude}" id="__element2">
							                    <iotele:status>
							                        <iotele:IoTStatusElement highSeverity="{DYN_ENT_com_sap_appiot_eventtypes__StandardEventType/High}" mediumSeverity="{DYN_ENT_com_sap_appiot_eventtypes__StandardEventType/Medium}" lowSeverity="{DYN_ENT_com_sap_appiot_eventtypes__StandardEventType/Low}" id="__element0"/>
							                    </iotele:status>
							                    <iotele:thingcard>
							                        <iotele:IoTThingCardElement thingCardImage="{ThingId}" SingleThingCardHeaderPosition1="{ThingName}" SingleThingCardHeaderPosition2="{ThingId}" SingleThingCardHeaderPosition3="{ThingType}" SingleThingCardContactInfo1="{BusinessPartnerName}" SingleThingCardContactInfo2="{BPMobilePhoneNumber}" SingleThingCardContactInfo3="{BPLandlinePhoneNumber}" MultiThingCardHeaderPosition1="{ThingName}" MultiThingCardHeaderPosition2="{ThingId}" SingleThingCardEventsVisible="true" SingleThingCardContactsVisible="true" id="__element1" action:wiring="\{'singleThingCardHeaderSelect':\{'navigation':\{'routeName':'ThingAndChart_binded','keys':[\{'name':'ThingId','type':'Edm.String'\}]\}\}\}" singleThingCardFooterSelect="" singleThingCardHeaderSelect="action" multiThingCardRowSelect=""/>
							                    </iotele:thingcard></iotele:IoTMapElement>
							            </iot:things>
							        </iot:IoTMap>
							    </items>
							</VBox>
						</items>
					</HBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>