<mvc:View xmlns:html="http://www.w3.org/1999/xhtml"
          xmlns:mvc="sap.ui.core.mvc"
          xmlns:iot="sap.ui.iot.controls"
          xmlns:m="sap.m" xmlns:core="sap.ui.core"
          xmlns:suite="sap.suite.ui.commons" xmlns="sap.uxap"
          xmlns:semantic="sap.m.semantic"
          xmlns:layout="sap.ui.layout"
          xmlns:f="sap.ui.layout.form"
          xmlns:viz="sap.viz.ui5.controls"
          xmlns:iotele="sap.ui.iot.elements"
          xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
          controllerName="webapp.controller.ThingPage"
          displayBlock="true">
<m:Page id="ThingPage" title="{i18n>ThingPage}"
		showNavButton="true"
		navButtonPress="onNavBack"
		class="sapUiResponsiveContentPadding">
    <headerTitle>
      <ObjectPageHeader objectTitle="{thingPageModel>/detailsData/ThingName}" objectSubtitle="{thingPageModel>/detailsData/ThingName}" objectImageAlt="{i18n>thing}" id="ObjectPageLayoutHeaderTitle" isObjectIconAlwaysVisible="false" isObjectTitleAlwaysVisible="true" isObjectSubtitleAlwaysVisible="true" showPlaceholder="true"/>
    </headerTitle>
		<m:content>
        <m:Text text="{thingPageModel>/detailsData/ThingName}" />
        <m:Text text="{thingPageModel>/detailsData/ThingName}" />
        <m:Text text="{thingPageModel>/detailsData}" />
        <!--
			<List id="employeeList" headerText="{i18n>ThingPage}" items="{/Employees}">
				<items>
					<StandardListItem
						title="{FirstName} {LastName}"
						iconDensityAware="false"
						iconInset="false"/>
				</items>
			</List>
            -->
		</m:content>
	</m:Page>
</mvc:View>
