<mvc:View
  xmlns:html="http://www.w3.org/1999/xhtml"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:iot="sap.ui.iot.controls"
  xmlns:m="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:suite="sap.suite.ui.commons"
  xmlns="sap.m"
  xmlns:semantic="sap.m.semantic"
  xmlns:layout="sap.ui.layout"
  xmlns:f="sap.ui.layout.form"
  xmlns:viz="sap.viz.ui5.controls"
  xmlns:iotele="sap.ui.iot.elements"
  xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
  controllerName="webapp.controller.ThingPage"
  displayBlock="true">

  <Page title="Page" enableScrolling="false">
    <content>
      <!--<Text text="{thingPageModel>/detailsData/ThingName}" />-->
      <!--<Text text="{thingPageModel>/detailsData}" />-->
      <iot:IoTChart id="idChart">
        <iot:propertySetTypes>
          <iotele:IoTPropertySetTypeElement propertySetTypeName="gbs.leonardo.com.itelligencegroup.iot.demo.sensor:TI_SensorTag_CAPPABILITY">
            <iotele:measures>
              <iotele:IoTPropertyElement propertyName="sensorAccX"/>
            </iotele:measures>
          </iotele:IoTPropertySetTypeElement>
        </iot:propertySetTypes>
      </iot:IoTChart>
      <!--<iot:IoTChart id="idChart" noOfHours="24" isSliderVisible="true"/>-->
    </content>
  </Page>
</mvc:View>
